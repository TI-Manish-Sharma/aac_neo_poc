{
  "expo": {
    "name": "aac-neo-mobile",
    "slug": "aac-neo-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/aac-neo-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/aac-neo-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.technizeredge.aacneomobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/aac-neo-logo-wo-tagline.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/aac-neo-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@react-native-community/datetimepicker",
        {
          "android": {
            "timePicker": {
              "headerBackground": {
                "light": "#00D2E6"
              },
              "numbersSelectorColor": {
                "light": "#00D2E6"
              }
            }
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5f3bc81c-2cd7-4a3f-82e3-f253d6496f2f"
      }
    }
  }
}